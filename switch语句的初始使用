#include<stdio.h>
int main()
{
	int day=0;
	scanf("%d",&day);
	switch (day) {
		case 1:
			printf("星期一\n");
			break;
		case 2:
			printf("星期二\n");
			break;
		case 3:
			printf("星期3\n");
				break;
		case 4:
			printf("星期4\n");
				break;
		case 5:
			printf("星期5\n");
				break;
		case 6:
			printf("星期6\n");
				break;
		case 7:
			printf("星期7\n");
				break;
		default:
			printf("输入错误\n");
	}
	return 0;
	
}
